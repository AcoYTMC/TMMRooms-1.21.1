{
     "schemaVersion": 1,
     "id": "tmmrooms",
     "version": "${version}",
     "name": "TMMRooms",
     "description": "Allows TMM map builders to have more rooms in the map.",
     "authors": [
          "AcoYT"
     ],
     "contact": {
          "sources": "https://github.com/AcoYTMC/TMMRooms-1.21.1"
     },
     "license": "MIT License",
     "icon": "assets/tmmrooms/icon.png",
     "environment": "*",
     "entrypoints": {
          "main": [
               "net.acoyt.tmmrooms.TMMRooms"
          ],
          "cardinal-components": [
               "net.acoyt.tmmrooms.cca.RoomsComponents"
          ]
     },
     "mixins": [
          "tmmrooms.mixins.json"
     ],
     "depends": {
          "fabricloader": ">=0.16.10",
          "minecraft": "~1.21.1",
          "java": ">=21",
          "fabric-api": "*",
          "trainmurdermystery": "*"
     },
     "custom": {
          "cardinal-components": [
               "tmmrooms:room_count"
          ]
     }
}
